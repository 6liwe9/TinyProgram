/* pages/takeaway/takeaway.wxss */
page{
  background-color:rgb(240,240,240);
}
.swiper {
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.selector{
  background-color:lightpink;
  border-radius: 20rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-bottom: 10rpx; 
  padding: 10rpx;
  display: flex;
  justify-content:center;
}
.btn{
  background-color:lightblue;
  border-radius: 20rpx;
  margin: 20rpx; 
  padding: 10rpx;
  display: flex;
  justify-content:center;
}
.itemClass{
  background-color:white;
  margin-top:10rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  padding: 20rpx;
  border-radius:20rpx;
}
.line{
  background-color:rgb(248,248,248);
  height: 5rpx;
  margin: 10rpx;
}
.textView{
  display: flex;
  justify-content:center;
}
.textFont{
  font-size:30rpx;
  font-family: '黑体';
  text-align: center;
  color: gray;
}
.userimage{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%
}
.user{
  display: flex;
  justify-content:center;
  margin-top: 5rpx;
}
.navi{
  display: flex;
  justify-content:center;
  flex-direction: column;
}